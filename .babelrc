{
  "presets": [
    "@babel/preset-env",  
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties", 
      { 
        "loose": true 
      }
    ],
    [
      "@babel/plugin-proposal-private-methods", 
      { 
        "loose": true 
      }
    ], 
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": false,
        "helpers": true,
        "regenerator": true,
        "useESModules": true
      }
    ]
  ]
}